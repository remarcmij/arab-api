{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "rules": {
    "arrow-parens": false,
    "quotemark": [true, "single"],
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "no-console": {
      "severity": "warning",
      "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
    }
  },
  "jsRules": {},
  "rulesDirectory": []
}
