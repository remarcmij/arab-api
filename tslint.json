{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "rules": {
    "arrow-parens": false,
    "quotemark": [true, "single"],
    "semicolon": [true, "always"],
    "object-literal-sort-keys": false,
    "no-console": false,
    "variable-name": {
      "options": ["allow-leading-underscore"]
    }
  },
  "jsRules": {},
  "rulesDirectory": []
}
